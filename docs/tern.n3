        [ a sh:PropertyShape ;
            sh:path dwc:infraspecificEpithet ],
        [ a sh:PropertyShape ;
            sh:path dwc:kingdom ],
        [ a sh:PropertyShape ;
            sh:path dwc:nameAccordingTo ],
        [ a sh:PropertyShape ;
            sh:name "name according to ID" ;
            sh:path dwc:nameAccordingToID ],
        [ a sh:PropertyShape ;
            sh:name "name published in" ;
