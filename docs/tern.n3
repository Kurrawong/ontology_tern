        [ a sh:PropertyShape ;
            sh:name "cultivar epithet" ;
            sh:path dwc:cultivarEpithet ],
        [ a sh:PropertyShape ;
            sh:path dwc:family ],
        [ a sh:PropertyShape ;
            sh:name "generic name" ;
            sh:path dwc:genericName ],
        [ a sh:PropertyShape ;
            sh:path dwc:genus ],
        [ a sh:PropertyShape ;
            sh:name "higher classification" ;
            sh:path dwc:higherClassification ],
        [ a sh:PropertyShape ;
            sh:name "infrageneric epithet" ;
