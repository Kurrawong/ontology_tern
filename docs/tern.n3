            sh:path sosa:isFeatureOfInterestOf ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path tern:featureType ] .

