            owl:onProperty <http://rdfs.org/ns/void#inDataset> ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty sosa:hasSimpleResult ],
        [ a owl:Restriction ;
