    rdfs:subPropertyOf geosparql:hasGeometry ;
    skos:definition "Link a [CollectedSample](#CollectedSample) to the location [Point](https://w3id.org/tern/ontologies/loc/Point) of where it is stored." .

tern:scope a owl:DatatypeProperty ;
    rdfs:label "scope" ;
    rdfs:range xsd:string ;
    skos:definition "Describe the scope of something." .

tern:siteShape a rdf:Property ;
    rdfs:label "site shape" ;
    rdfs:domain tern:Site ;
    skos:definition "The shape of the site. The value is expected to be from a controlled vocabulary with a persistent URI identifier." .

tern:siteVisit a rdf:Property ;
    rdfs:label "site visit" ;
    rdfs:range tern:SiteVisit ;
    skos:definition "Link to a [Site Visit](#SiteVisit)." .

tern:stratum a rdf:Property ;
    rdfs:label "stratum" ;
    rdfs:subPropertyOf dcterms:type ;
    skos:definition "Stratum classification." ;
    skos:scopeNote "Value should be from a controlled vocabulary identified by a URI." .

tern:swPoint a rdf:Property ;
    rdfs:label "sw point" ;
    rdfs:range loc:Point ;
    rdfs:subPropertyOf geosparql:hasGeometry ;
    skos:definition "The south-west point of the subject." .

tern:transectDirection a rdf:Property ;
    rdfs:label "transect direction" ;
    rdfs:domain tern:Transect ;
    rdfs:range xsd:string ;
    skos:definition "Describe the direction of the transect." ;
    skos:example "S-W" .

tern:transectEndPoint a rdf:Property ;
    rdfs:label "transect end point" ;
    skos:definition "Link to a [tern-loc:Point](https://w3id.org/tern/ontologies/loc/Point) representing the end of the transect." .

tern:transectStartPoint a rdf:Property ;
    rdfs:label "transect start point" ;
    rdfs:domain tern:Transect ;
    skos:definition "Link to a [tern-loc:Point](https://w3id.org/tern/ontologies/loc/Point) representing the start of the transect." .

tern:wasSubActivityOf a rdf:Property ;
    rdfs:label "was sub activity of" ;
    skos:definition "Link to parent activity." .

tern:CollectedSample a owl:Class ;
    rdfs:label "Collected Sample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dwc:materialSampleID ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass loc:Point ;
            owl:onProperty tern:sampleStorageLocation ],
        dwc:MaterialSample,
        tern:Sample ;
    skos:definition "A [Collected Sample](#CollectedSample) is a [Sample](#Sample) which has been collected in the field. " ;
    skos:example "Soil sample or plant sample from the field." .

tern:Instrument a owl:Class ;
    rdfs:label "Instrument" ;
    rdfs:subClassOf ssn:System ;
    skos:definition "A thing to measure or produce a sample." .

tern:ObservableProperty a owl:Class ;
    rdfs:label "Observable Property" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( skos:ConceptScheme skos:Collection owl:Ontology ) ] ;
            owl:onProperty tern:globalVocabulary ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty data:vocabulary ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty tern:globalVocabulary ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass skos:Concept ;
            owl:onProperty tern:globalValue ],
        [ a owl:Restriction ;
            owl:onClass tern:Parameter ;
            owl:onProperty rdf:value ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        sosa:ObservableProperty ;
    skos:definition "<i>Observable Property</i> - An observable quality (property, characteristic) of a <a href=\"http://w3.org/ns/sosa/FeatureOfInterest\">FeatureOfInterest</a>." .

tern:PropertyValue a owl:Class ;
    rdfs:label "Property Value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty schema:description ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty schema:name ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass tern:Result ;
            owl:onProperty sosa:hasResult ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        schema:PropertyValue ;
    skos:definition "A property-value pair. Use the [schema:name](http://schema.org/name) property for the name of the property. If there is an additional human-readable version of the value, put that into the [schema:description](http://schema.org/description) property." ;
    skos:example """@prefix tern: <https://w3id.org/tern/ontologies/tern/> .
@prefix schema: <http://schema.org/> .
@prefix data: <http://linked.data.gov.au/def/datatype/> .
@prefix sosa: <http://w3.org/ns/sosa/> .

<example> a tern:PropertyValue ;
	schema:name "AUSPLOTS_RANGELANDS.SITE.PROPERTY" ;
	schema:description "The property name" ;
	sosa:hasResult [
		a data:Text ;
		data:value "Great Victoria Desert Nature Reserve" ;
	] .""" .

tern:Sampler a owl:Class ;
    rdfs:label "Sampler" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tern:Sampling ;
            owl:onProperty sosa:madeSampling ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Procedure ;
            owl:onProperty ssn:implements ],
        sosa:Sampler,
        tern:Instrument ;
    skos:definition "*Sampler* - A device that is used by, or implements, a (Sampling) [Procedure](#Procedure) to create or transform one or more samples." .

tern:centroidPoint a rdf:Property ;
    rdfs:label "centroid point" ;
    rdfs:range loc:Point ;
    rdfs:subPropertyOf geosparql:hasGeometry ;
    skos:definition "The centroid point of the subject." .

tern:dateCommissioned a rdf:Property ;
    rdfs:label "date commissioned" ;
    skos:definition "The date when something was commissioned, e.g. a [Site](#Site)." .

tern:dateDecommissioned a rdf:Property ;
    rdfs:label "date decommissioned" ;
    skos:definition "The date when something was decommissioned, e.g. a [Site](#Site)." .

tern:globalValue a rdf:Property ;
    rdfs:label "global value" ;
    skos:definition "The global controlled vocabulary concept (in the closed system)." .

tern:hasSamplingPoint a rdf:Property ;
    rdfs:label "has sampling point" ;
    rdfs:range tern:SamplingPoint ;
    skos:definition "Link to a [SamplingPoint](#SamplingPoint)." .

tern:length a rdf:Property ;
    rdfs:label "length" ;
    skos:definition "The measurement or extent of something from end to end." .

tern:locationDescription a rdf:Property ;
    rdfs:label "location description" ;
    skos:definition "The description of the location." .

tern:site a rdf:Property ;
    rdfs:label "site" ;
    rdfs:range tern:Site ;
    skos:definition "Link to a [Site](#Site)." .

tern:siteDescription a rdf:Property ;
    rdfs:label "site description" ;
    skos:definition "The description of the site." .

tern:taxon a rdf:Property ;
    rdfs:label "taxon" ;
    skos:definition "Taxon classification." ;
    skos:scopeNote "Value should be from a controlled vocabulary identified by a URI." .

tern:width a rdf:Property ;
    rdfs:label "width" ;
    skos:definition "The measurement or extent of something from side to side." .

tern:FixedPlatform a owl:Class ;
    rdfs:label "Fixed Platform" ;
    rdfs:subClassOf tern:Platform ;
    skos:definition "A fixed platform based on land." .

tern:SamplingPoint a owl:Class ;
    rdfs:label "Sampling Point" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass data:QuantitativeMeasure ;
            owl:onProperty loc:altitude ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass data:QuantitativeMeasure ;
            owl:onProperty loc:depth ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass data:QuantitativeMeasure ;
            owl:onProperty loc:elevation ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty dcterms:identifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Transect ;
            owl:onProperty tern:isSamplingPointOf ],
        [ a owl:Restriction ;
            owl:onClass data:QuantitativeMeasure ;
            owl:onProperty loc:latitude ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass data:QuantitativeMeasure ;
            owl:onProperty loc:longitude ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange loc:wktLiteral ;
            owl:onProperty geosparql:asWKT ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        loc:Point ;
    skos:definition "A point in space where an [Observation](#Observation) or [Sampling](#Sampling) occurred. " .

tern:Sensor a owl:Class ;
    rdfs:label "Sensor" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tern:ObservableProperty ;
            owl:onProperty sosa:observes ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Observation ;
            owl:onProperty sosa:madeObservation ],
        sosa:Sensor,
        tern:Instrument ;
    skos:definition "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms." .

tern:SiteVisit a owl:Class ;
    rdfs:label "Site Visit" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty prov:endedAtTime ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty prov:startedAtTime ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:CollectedSample ;
            owl:onProperty sosa:hasSample ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:PropertyValue ;
            owl:onProperty schema:additionalProperty ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( tern:Observation tern:Sampling ) ] ;
            owl:onProperty tern:hadSubActivity ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty tern:locationDescription ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty tern:siteDescription ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty dcterms:identifier ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty rdfs:label ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass tern:Site ;
            owl:onProperty tern:site ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        prov:Activity ;
    skos:definition "A discrete time-bounded visit to a site, during which sampling or observation activities are undertaken." .

tern:Transect a owl:Class ;
    rdfs:label "Transect" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tern:SamplingPoint ;
            owl:onProperty tern:hasSamplingPoint ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty tern:transectDirection ],
        [ a owl:Restriction ;
            owl:onClass loc:LineString ;
            owl:onProperty geosparql:hasGeometry ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass loc:Point ;
            owl:onProperty tern:transectEndPoint ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass loc:Point ;
            owl:onProperty tern:transectStartPoint ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        tern:Sample ;
    skos:definition "A straight line or narrow section through an object or natural feature or across the earth's surface, along which observations are made or measurements taken." .

tern:globalVocabulary a rdf:Property ;
    rdfs:label "global vocabulary" ;
    skos:definition "The global vocabulary which the global:value belongs to (in the closed system)." .

tern:Platform a owl:Class ;
    rdfs:label "Platform" ;
    rdfs:subClassOf sosa:Platform ;
    skos:definition "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms." ;
    skos:example "A post, buoy, vehicle, ship, aircraft, satellite, cell-phone, human or animal may act as platforms for (technical or biological) sensors or actuators." .

tern:Procedure a owl:Class ;
    rdfs:label "Procedure" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty data:vocabulary ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Input ;
            owl:onProperty ssn:hasInput ],
        [ a owl:Restriction ;
            owl:onClass tern:Method ;
            owl:onProperty rdf:value ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        sosa:Procedure ;
    skos:definition "*Procedure* - A workflow, protocol, plan, algorithm, or computational method specifying how to make an [Observation](#Observation), create a [Sample](#Sample), or make a change to the state of the world (via an [Actuator](http://w3.org/ns/sosa/Actuator). A [Procedure](#Procedure) is re-usable, and might be involved in many [Observations](#Observation), [Samplings](#Samplings), or [Actuations](http://w3.org/ns/sosa/Actuation). it explains the steps to be carried out to arrive at reproducible [Results](#Result)." ;
    skos:scopeNote "[Input](#Input) can be used to capture any input values for a procedure, such as the input value of the basal wedge to estimate the basal area per hectare." .

tern:Sampling a owl:Class ;
    rdfs:label "Sampling" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern-org:Person ;
            owl:onProperty prov:wasAttributedTo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:SamplingPoint ;
            owl:onProperty tern:hasSamplingPoint ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty rdfs:label ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass tern:Procedure ;
            owl:onProperty sosa:usedProcedure ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass tern:Sample ;
            owl:onProperty sosa:hasResult ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        prov:Activity,
        sosa:Sampling ;
    skos:definition "*Sampling* - An act of [Sampling](#Sampling) carries out a (Sampling) [Procedure](#Procedure) to create or transform one or more [Samples](#Sample)." ;
    skos:scopeNote """Crushing a rock sample in a ball mill.
Digging a pit through a soil sequence.
Dividing a field site into quadrants.
Drawing blood from a patient.
Drilling an observation well.
Establishing a station for environmental monitoring.
Registering an image of the landscape.
Sieving a powder to separate the subset finer than 100-mesh.
Selecting a subset of a population.
Splitting a piece of drill-core to create two new samples.
Taking a diamond-drill core from a rock outcrop.""" .

data:QuantitativeMeasure rdfs:subClassOf tern:Result .

tern:Observation a owl:Class ;
    rdfs:label "Observation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern-org:Person ;
            owl:onProperty prov:wasAttributedTo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Site ;
            owl:onProperty tern:site ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:SiteVisit ;
            owl:onProperty tern:wasSubActivityOf ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Result ;
            owl:onProperty sosa:hasResult ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty rdfs:label ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass time:Instant ;
            owl:onProperty sosa:phenomenonTime ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass sosa:FeatureOfInterest ;
            owl:onProperty sosa:hasFeatureOfInterest ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass tern:ObservableProperty ;
            owl:onProperty sosa:observedProperty ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass tern:Procedure ;
            owl:onProperty sosa:usedProcedure ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:dateTime ;
            owl:onProperty sosa:resultTime ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        prov:Activity,
        sosa:Observation ;
    skos:definition "*Observation* - Act of carrying out an (Observation) [Procedure](#Procedure) to estimate or calculate a value of a property of a [FeatureOfInterest](http://w3.org/ns/sosa/FeatureOfInterest). Links to a [Sensor](http://w3.org/ns/sosa/Sensor) to describe what made the [Observation](#Observation) and how; links to an [ObservableProperty](#ObservableProperty) to describe what the result is an estimate of, and to a [FeatureOfInterest](http://w3.org/ns/sosa/FeatureOfInterest) to detail what that property was associated with." .

tern:Sample a owl:Class ;
    rdfs:label "Sample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern-org:Person ;
            owl:onProperty prov:wasAttributedTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Observation ;
            owl:onProperty sosa:isFeatureOfInterestOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Sample ;
            owl:onProperty sosa:hasSample ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Sample ;
            owl:onProperty sosa:isSampleOf ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Sampling ;
            owl:onProperty sosa:isResultOf ],
        sosa:Sample ;
    skos:definition "*Sample* - Feature which is intended to be representative of a [FeatureOfInterest](http://w3.org/ns/sosa/FeatureOfInterest) on which [Observations](#Observation) may be made." .

tern:Result a owl:Class ;
    rdfs:label "Result" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tern-org:Person ;
            owl:onProperty prov:wasAttributedTo ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( tern:Observation tern:Sampling ) ] ;
            owl:onProperty sosa:isResultOf ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty sosa:isResultOf ],
        sosa:Result ;
    skos:definition "*Result* - The result of an [Observation](#Observation), [Actuation](http://w3.org/ns/sosa/Actuation), or act of [Sampling](#Sampling). To store an observation's simple result value one can use the [hasSimpleResult](http://w3.org/ns/sosa/hasSimpleResult) property." ;
    skos:example "The value 20 as the height of a certain tree together with the unit, e.g., Meter." .

tern:Site a owl:Class ;
    rdfs:label "Site" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:date ;
            owl:onProperty tern:dateDecommissioned ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty tern:cardinalDirection ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty tern:locationDescription ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass xsd:string ;
            owl:onProperty tern:siteDescription ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass skos:Concept ;
            owl:onProperty tern:siteShape ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass loc:Point ;
            owl:onProperty tern:centroidPoint ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass loc:Point ;
            owl:onProperty tern:swPoint ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass loc:Polygon ;
            owl:onProperty tern:polygon ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass tern:Dimension ;
            owl:onProperty tern:dimension ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:onClass xsd:date ;
            owl:onProperty tern:dateCommissioned ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty dcterms:identifier ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass xsd:string ;
            owl:onProperty rdfs:label ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass <http://www.w3.org/ns/dcat#Dataset> ;
            owl:onProperty <http://www.w3.org/ns/dcat#dataset> ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:allValuesFrom geosparql:Feature ;
            owl:onProperty geosparql:sfWithin ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:CollectedSample ;
            owl:onProperty sosa:hasSample ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Procedure ;
            owl:onProperty tern:locationProcedure ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:PropertyValue ;
            owl:onProperty schema:additionalProperty ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Site ;
            owl:onProperty dcterms:hasPart ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:Site ;
            owl:onProperty dcterms:isPartOf ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern:SiteVisit ;
            owl:onProperty tern:siteVisit ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( loc:Point loc:Polygon ) ] ;
            owl:onProperty geosparql:hasGeometry ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty prov:generatedAtTime ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty tern:hasEnvironmentalCharacteristic ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass data:QuantitativeMeasure ;
            owl:onProperty tern:area ],
        tern:Platform,
        tern:Sample ;
    skos:definition "Location where observations may be made. This is a subclass of [Sample](#Sample) because a site should be designed to be representative of an environmental system (which may be an ecosystem or bioregion) or zone (which may be a zone such as a parcel or tract)." ;
