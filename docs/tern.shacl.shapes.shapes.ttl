# baseURI: https://w3id.org/tern/shacl/tern/
# imports: http://datashapes.org/dash
# imports: https://w3id.org/tern/ontologies/tern/
# prefix: tern-shape

@prefix dash: <http://datashapes.org/dash#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix tern-shape: <https://w3id.org/tern/shacl/tern/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

tern-shape:
  a owl:Ontology ;
  owl:imports <http://datashapes.org/dash> ;
  owl:imports <https://w3id.org/tern/ontologies/tern/> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
tern-shape:Attribute
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Attribute" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:closed true ;
  sh:ignoredProperties (
      rdf:type
    ) ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://purl.org/dc/terms/type> ;
      sh:maxCount 1 ;
      sh:name "type" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://rdfs.org/ns/void#inDataset> ;
      sh:class <http://rdfs.org/ns/void#Dataset> ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://www.w3.org/ns/sosa/hasResult> ;
      sh:class <https://w3id.org/tern/ontologies/tern/Result> ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "has result" ;
      sh:nodeKind sh:BlankNodeOrIRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <http://www.w3.org/ns/sosa/hasSimpleResult> ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "has simple result" ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <https://w3id.org/tern/ontologies/tern/attribute> ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path <https://w3id.org/tern/ontologies/tern/isAttributeOf> ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass <https://w3id.org/tern/ontologies/tern/Attribute> ;
.
